    {{#each this}} 
        {{#if this.title}}    
            {{> components/Header/NavigationFlyout/NavigationFlyoutItem parentselected=../selected _fromNav=../navtype ~}} 
            <div class="pr-4 leading-10 text-gray-600">|</div>
        {{/if}}
        
    {{/each~}}
</ul>